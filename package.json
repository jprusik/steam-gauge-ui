{
  "name": "steam-gauge-ui",
  "version": "0.1.0",
  "description": "Steam Gauge allows you to get a customizable summary of your Steam account, including game values, install sizes, and play time.",
  "keywords": [
    "Steam",
    "Steam profile",
    "Steam calculator",
    "steam stats",
    "Steam games",
    "steam account",
    "steam tables",
    "steam profile size",
    "steam account value",
    "steam play time",
    "steam friends",
    "steam friends games in common"
  ],
  "homepage": "https://mysteamgauge.com",
  "bugs": "mailto:jon@mysteamgauge.com",
  "author": {
    "name": "Jonathan Prusik",
    "email": "jprusik@classynemesis.com",
    "url": "https://jonathanprusik.com"
  },
  "repository": {
    "type": "git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-csv": "^2.0.3",
    "react-dom": "^17.0.1",
    "react-meta-tags": "^0.4.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-table": "^7.6.3"
  },
  "devDependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o build/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o build/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
